PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX glycordf: <http://purl.jp/bio/12/glyco/glycan/>

SELECT DISTINCT ?structure ?taxon
WHERE {
    ?structure rdf:type glycordf:saccharide .
    ?refcompound rdf:type glycordf:referenced_compound .
    ?refcompound glycordf:has_glycan ?structure .
    ?refcompound glycordf:is_from_source ?source .
    ?source glycordf:has_taxon ?taxon
}



